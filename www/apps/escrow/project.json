{
  "name": "escrow-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/escrow/src",
  "prefix": "casper-escrow",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/escrow",
        "index": "apps/escrow/src/index.html",
        "main": "apps/escrow/src/main.ts",
        "polyfills": "apps/escrow/src/polyfills.ts",
        "tsConfig": "apps/escrow/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/escrow/src/favicon.ico",
          "apps/escrow/src/assets"
        ],
        "styles": [
          "apps/escrow/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/escrow/src/environments/environment.ts",
              "with": "apps/escrow/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "escrow-app:build:production"
        },
        "development": {
          "browserTarget": "escrow-app:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "proxyConfig": "apps/escrow/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "escrow-app:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/escrow/**/*.ts",
          "apps/escrow/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/escrow"
      ],
      "options": {
        "jestConfig": "apps/escrow/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}