{
  "version": 2,
  "name": "casper-escrow",
  "installCommand": "npm install",
  "buildCommand": "npm run build:prod",
  "outputDirectory": "dist/apps/escrow/",
  "builds": [
    {
      "src": "./www/dist/apps/api/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "./www/dist/apps/api/main.js"
    }
  ]
}